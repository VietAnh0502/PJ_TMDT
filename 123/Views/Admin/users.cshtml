@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model List<string>
<div class="card mb-4">
  <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Th√¥ng tin users</h6>
  </div>
  <div class="card-body">
      <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                  <tr>
                      <th>Name</th>
                      <th>Position</th>
                      <th>Office</th>
                      <th>Age</th>
                      <th>Start date</th>
                      <th>Salary</th>
                  </tr>
              </thead>
              <tfoot>
                  @foreach (var user in Model)
                  {
                  <tr>
                      <th>user.username</th>
                  </tr>
                  }
              </tfoot>
              <tbody>
                  <tr>
                      <td>Tiger Nixon</td>
                      <td>System Architect</td>
                      <td>Edinburgh</td>
                      <td>61</td>
                      <td>2011/04/25</td>
                      <td>$320,800</td>
                  </tr>
              </tbody>
          </table>
      </div>
  </div>
</div>
